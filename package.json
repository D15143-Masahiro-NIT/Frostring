{
  "name": "frostring",
  "displayName": "Frostring",
  "description": "This extension supports addition frozen string literal comment to the top of ruby files.",
  "version": "0.0.6",
  "publisher": "Masahiro",
  "author": "Masahiro",
  "icon": "Logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/watanabemasahiro128/Frostring.git"
  },
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Linters"
  ],
  "activationEvents": [
    "onLanguage:ruby"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "contributes": {
    "languages": [
      {
        "id": "ruby",
        "extensions": [
          ".rb"
        ]
      },
      {
        "id": "gemfile",
        "filenamePatterns": [
          "Gemfile"
        ]
      }
    ]
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.42",
    "@types/vscode": "^1.68.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.3",
    "vscode-test": "^1.6.1"
  }
}
